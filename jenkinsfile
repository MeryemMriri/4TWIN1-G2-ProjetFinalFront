
pipeline{
  agent any



  stages {

  stage('install dependencies') {
              steps {
                  sh 'npm install'

              }
          }
 


  }
  stage('Unit Test') {
      steps{
        
        script {
         sh('npm test')
        }
      }
    }
  

  stage('MVN SONARQUBE') {
            steps {
                sh 'mvn sonar:sonar -Dsonar.login=admin -Dsonar.password=mimomimi99 -Dmaven.test.skip=true';
            }
        }

}

